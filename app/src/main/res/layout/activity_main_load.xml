<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainLoadActivity">
    <ImageView
            android:layout_width="238dp"
            android:layout_height="0dp" app:srcCompat="@drawable/main_logo"
            android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="175dp" android:layout_marginBottom="95dp"
            app:layout_constraintBottom_toTopOf="@+id/temp_start_button"/>
    <Button
            android:text="시작하기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/temp_start_button"
            app:layout_constraintTop_toBottomOf="@+id/imageView" android:layout_marginBottom="177dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

</android.support.constraint.ConstraintLayout>